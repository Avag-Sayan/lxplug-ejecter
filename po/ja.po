#: ../plugins/ejecter/ejecter.c:169
msgid "Drive was removed without ejecting"
msgstr "ドライブが取り外しを実行する前に抜き取られました"

#: ../plugins/ejecter/ejecter.c:169
msgid "Please use menu to eject before removal"
msgstr "抜き取る前にはメニューを使用して取り外してください"

#: ../plugins/ejecter/ejecter.c:197
#, c-format
msgid "%s has been ejected"
msgstr "%s が取り外されました"

#: ../plugins/ejecter/ejecter.c:198
msgid "It is now safe to remove the device"
msgstr "安全にデバイスを抜き取ることができます"

#: ../plugins/ejecter/ejecter.c:203
#, c-format
msgid "Failed to eject %s"
msgstr "%s の取り外しに失敗しました"

#: ../plugins/ejecter/ejecter.c:555
msgid "Select a drive in menu to eject safely"
msgstr "安全に取り外すドライブをメニューから選択します"

#: ../plugins/ejecter/ejecter.c:612 ../plugins/ejecter/ejecter.c:622
msgid "Ejecter"
msgstr "Ejecter"

#: ../plugins/ejecter/ejecter.c:614
msgid "Hide icon when no devices"
msgstr "デバイスがないときはアイコンを隠す"

#: ../plugins/ejecter/ejecter.c:623
msgid "Ejects mounted drives"
msgstr "マウントされたドライブを取り外します"

#~ msgid "No ejectable devices"
#~ msgstr "No ejectable devices"
